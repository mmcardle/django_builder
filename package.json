{
  "name": "djangobuilder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e_build": "yarn build --mode=development --dest=dist",
    "test:e2e_serve": "firebase emulators:start --project development --only firestore,hosting",
    "test:e2e": "nightwatch -e chrome",
    "ci": "start-server-and-test test:e2e_serve http://localhost:8082 test:e2e"
  },
  "dependencies": {
    "firebase": "^8.2.7",
    "firebase-tools": "^9.4.0",
    "highlight.js": "^10.6.0",
    "raw-loader": "^4.0.2",
    "tar-js": "^0.3.0",
    "vue": "^2.6.12",
    "vue-analytics": "^5.16.4",
    "vue-highlight.js": "^3.1.0",
    "vue-router": "^3.5.1",
    "vuetify": "^2.4.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@mdi/js": "^5.9.55",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "chromedriver": "^88.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.6.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.8",
    "geckodriver": "^1.22.1",
    "jest-raw-loader": "^1.0.1",
    "nightwatch": "^1.5.1",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "selenium-server": "^3.141.59",
    "start-server-and-test": "^1.12.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "vue-cli-plugin-vuetify": "^2.1.0",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "plugins": [
      "vuetify"
    ],
    "rules": {
      "vuetify/no-deprecated-classes": "error",
      "vuetify/no-legacy-grid": "error",
      "no-console": "off",
      "no-empty-pattern": "error",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "argsIgnorePattern": "^_|^unused$",
          "varsIgnorePattern": "^_.*$"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
