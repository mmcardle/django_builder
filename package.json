{
  "name": "djangobuilder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "test:unit": "vitest --run",
    "test:e2e_chrome": "nightwatch --headless -e chrome",
    "test:e2e_firefox": "nightwatch --headless -e firefox",
    "test:e2e": "yarn test:e2e_chrome && yarn test:e2e_firefox",
    "test:e2e_serve": "firebase emulators:start --project development --only firestore,hosting",
    "pree2e": "yarn bic",
    "e2e": "start-server-and-test test:e2e_serve http://localhost:8082 test:e2e_chrome",
    "preci": "yarn bic",
    "ci": "start-server-and-test test:e2e_serve http://localhost:8082 test:e2e"
  },
  "dependencies": {
    "firebase": "^9.1.0",
    "firebase-tools": "^9.19.0",
    "highlight.js": "^11.2.0",
    "raw-loader": "^4.0.2",
    "tar-js": "^0.3.0",
    "vue": "^2.7.13",
    "vue-gtag": "1.16.1",
    "vue-highlight.js": "^4.0.0-beta",
    "vue-router": "^3.0.1",
    "vuetify": "^2.2.23",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^6.1.95",
    "@mdi/js": "^6.1.95",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitest/ui": "^0.24.3",
    "@vue/compiler-dom": "^3.2.41",
    "@vue/test-utils": "^1.3.0",
    "autoprefixer": "^10.4.8",
    "babel-core": "^6.26.3",
    "build-if-changed": "^1.5.5",
    "chromedriver": "101",
    "eslint": "8",
    "eslint-plugin-vue": "8",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "geckodriver": "^2.0.4",
    "jsdom": "^20.0.1",
    "nightwatch": "^1.3.6",
    "sass": "^1.54.5",
    "selenium-server": "^3.141.59",
    "start-server-and-test": "^1.11.2",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.1.0",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^2.5.4",
    "vite-plugin-vue2": "1.9.0",
    "vitest": "^0.24.3",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.4.3"
  },
  "bic": [
    "src"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "es2021": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "plugins": [
      "vuetify"
    ],
    "rules": {
      "vuetify/no-deprecated-classes": "error",
      "vuetify/no-legacy-grid": "error",
      "no-console": "off",
      "no-empty-pattern": "error",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "argsIgnorePattern": "^_|^unused$",
          "varsIgnorePattern": "^_.*$"
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
