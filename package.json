{
  "name": "djangobuilder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e_chrome": "nightwatch --headless -e chrome",
    "test:e2e_firefox": "nightwatch --headless -e firefox",
    "test:e2e": "yarn test:e2e_chrome && yarn test:e2e_firefox",
    "test:e2e_serve": "firebase emulators:start --project development --only firestore,hosting",
    "pree2e": "yarn bic",
    "e2e": "start-server-and-test test:e2e_serve http://localhost:8082 test:e2e_chrome",
    "preci": "yarn bic",
    "ci": "start-server-and-test test:e2e_serve http://localhost:8082 test:e2e"
  },
  "dependencies": {
    "firebase": "^9.1.0",
    "firebase-tools": "^9.19.0",
    "highlight.js": "^11.2.0",
    "raw-loader": "^4.0.2",
    "tar-js": "^0.3.0",
    "vue": "^2.5.21",
    "vue-gtag": "1.16.1",
    "vue-highlight.js": "^4.0.0-beta",
    "vue-router": "^3.0.1",
    "vuetify": "^2.2.23",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^6.1.95",
    "@mdi/js": "^6.1.95",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.2.4",
    "build-if-changed": "^1.5.5",
    "chromedriver": "^94.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "geckodriver": "^2.0.4",
    "jest-raw-loader": "^1.0.1",
    "nightwatch": "^1.3.6",
    "sass": "^1.26.3",
    "sass-loader": "^10.1.1",
    "selenium-server": "^3.141.59",
    "start-server-and-test": "^1.11.2",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.1.0",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.4.3"
  },
  "bic": [
    "src"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "plugins": [
      "vuetify"
    ],
    "rules": {
      "vuetify/no-deprecated-classes": "error",
      "vuetify/no-legacy-grid": "error",
      "no-console": "off",
      "no-empty-pattern": "error",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "argsIgnorePattern": "^_|^unused$",
          "varsIgnorePattern": "^_.*$"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
